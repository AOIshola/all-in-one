{
    "manifest_version": 3,
    "name": "Tab Manager",
    "description": "Tab Manager is a interface to manage all open tabs in a browser window. Keep tabs of your tabs and manage them all in a single tab. You should try it out.",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png"
    } ,
    "background": {
      "service_worker": "background.js",
      "web_accessible_resources": [
        {
          "resources": ["tablist.html"],
          "icons": ["icon.png"]
        }
      ]
    },
    "permissions": [
      "tabs",
      "storage",
      "sessions",
      "contextMenus",
      "favicon",
      "alarms",
      "notifications"
    ],
    "web_accessible_resources": [
      {
        "resources": ["tablist.html", "tablist.js", "_favicon/*"],
        "matches": ["<all_urls>"]
      }
    ]
}  