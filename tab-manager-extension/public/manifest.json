{
    "manifest_version": 3,
    "name": "Tab Manager",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "tabs",
      "storage",
      "sessions",
      "contextMenus"
    ],
    "web_accessible_resources": [
      {
        "resources": ["tablist.html", "tablist.js"],
        "matches": ["<all_urls>"]
      }
    ]
}  